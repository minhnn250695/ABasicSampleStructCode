{
    "extends": "tslint:recommended",
    "rules": {
        "align": false,
        "arrow-parens":false,
        "curly": false,
        "eofline": false,
        "max-line-length": {
            "options": [
                120
            ]
        },
        "member-access": false,
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "no-empty": false,
        "object-literal-sort-keys": false,
        "one-line": false,
        "quotemark":false,
        "trailing-comma": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    }
}